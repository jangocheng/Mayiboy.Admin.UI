<?xml version="1.0" encoding="utf-8" ?>
<Framework.Mayiboy.Logging>
  <!-- 日志组件全局配置 -->
  <Global IsLog="True" ThrowException="false" IsAsyn="true" StartDebugConsole="true" InnerLogPath="D:\applogs\Project.Name.dev\Logging\"/>

  <!-- 日志组件用户自定义变量 -->
  <Variable>
    <var name="AppId" value="000000"/>
    <var name="LogDir" value="D:\applogs\Project.Name.dev\"/>
    <var name="LogDateDir" value="D:\applogs\Project.Name.dev\{year}\{month}\{day}\"/>
    <var name="MailServer" value="smtp.***.com"/>
    <var name="FromMail" value="frommail@***.com"/>
    <var name="ToMail" value="tomaile@***.com"/>
    <var name="MailAccount" value="mailaccount@***.com"/>
    <var name="MailPassword" value="********"/>
    <var name="SysName" value="projectname.dev Framework.Mayiboy.Logging"/>
  </Variable>

  <Targets>
  </Targets>

  <!-- 日志对象 -->
  <Loggers>
    <Logger name="CachingLogger" ConfigMode="Classics" IsLog="true"></Logger>
  </Loggers>
</Framework.Mayiboy.Logging>